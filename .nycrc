{
  "all": true,
  "check-coverage": true,
  "include": [
    "test/*.js",
  ],
  "exclude": [
    "lib/index.js"
  ],
  "reporter": [
    "text-summary"
  ],
  "no-watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  },
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
